<!-- contact area start -->
<div class="contact__area">
  <div class="container">
    <div class="row">
      <div class="col-xl-12 col-lg-12 col-md-12">
        <div class="contact__content text-center mb-140">
          <h2>{{users.name}}</h2>
          <p>{{users.email}}<br />{{users.phone}}</p>
        </div>
      </div>
    </div>

    <div class="contact-wrapper align-items-center">

      <div class="row">

        {{! <div class="col-xl-6 col-lg-6 col-md-12 col-md-6"> }}
        <h4>Your saved adresses</h4>

        {{! <div class="table-responsive">
              
          </div> }}
